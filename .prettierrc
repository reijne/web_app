{
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "trailingComma": "es5",
    "tabWidth": 4,
    "indentSize": 4,
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "importOrder": [
        "^react$",
        "^react/(.*)$",
        "<THIRD_PARTY_MODULES>",
        "^(../)(.*)$",
        "^(./)(.*)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
